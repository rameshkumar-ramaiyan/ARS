{   
    //you can define multiple editors   
    propertyEditors: [      
        {
            /*this must be a unique alias*/ 
            alias: "UsdaDocTypeList",
            /*the name*/
            name: "DocType List",
			
            /*the html file we will load for the editor*/
            editor: {
                view: "~/App_Plugins/UsdaDocTypeList/views/usda.doctype.list.html"
            }
        }
    ]
    ,
    //array of files we want to inject into the application on app_start
    javascript: [
		'~/App_Plugins/UsdaDocTypeList/js/usda.doctype.list.js'
    ]
}